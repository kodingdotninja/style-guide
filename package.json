{
  "name": "@kodingdotninja/style-guide",
  "description": "ESLint and Prettier style guide for various Koding Ninja projects ðŸ¤™",
  "version": "4.0.0",
  "author": "Griko Nibras <griko@koding.ninja>",
  "repository": "https://github.com/kodingdotninja/style-guide",
  "homepage": "https://github.com/kodingdotninja/style-guide",
  "bugs": "https://github.com/kodingdotninja/style-guide/issues",
  "files": [
    "eslint/**/*.js",
    "prettier/**/*.js",
    "tsconfig/*.json",
    "*.json",
    "index.js",
    "**/*.d.ts"
  ],
  "scripts": {
    "mod:core": "node scripts/prepare-core.js",
    "mod:github": "node scripts/prepare-github.js",
    "mod:typedefs": "node scripts/update-eslint-typedefs.mjs",
    "prepublish": "pnpm run mod:typedefs",
    "release": "./release"
  },
  "sideEffects": false,
  "dependencies": {
    "@babel/core": "^7.22.10",
    "@babel/eslint-parser": "^7.22.10",
    "@rushstack/eslint-patch": "^1.3.3",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prefer-arrow-functions": "^3.1.4",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-unicorn": "^48.0.1",
    "eslint-plugin-unused-imports": "^3.0.0"
  },
  "peerDependencies": {
    "eslint": ">=8",
    "prettier": ">=2 || >=3",
    "typescript": ">=4 || >=5"
  },
  "optionalDependencies": {
    "@next/eslint-plugin-next": "^13.4.19",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-playwright": "^0.15.3",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-testing-library": "^6.0.0"
  },
  "devDependencies": {
    "@types/eslint": ">=8",
    "@types/node": ">=18",
    "globby": "^13.2.2"
  },
  "keywords": [
    "eslint-config",
    "eslint",
    "prettier-config",
    "prettier",
    "style-guide",
    "tsconfig"
  ],
  "publishConfig": {
    "access": "public"
  },
  "prettier": "./prettier",
  "license": "MPL"
}
